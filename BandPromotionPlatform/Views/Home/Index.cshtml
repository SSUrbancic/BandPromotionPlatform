@{
    ViewData["Title"] = "Home Page";
}
<body style="background-color: ThreeDDarkShadow">
    <div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="5000" style="background-color:darkviolet">
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>
        <div class="carousel-inner" role="listbox">
            <div class="item active">
                <img src="https://yt3.ggpht.com/-q5Ff-jKaDxg/AAAAAAAAAAI/AAAAAAAAAAA/Y9Usc0nl2f4/s288-mo-c-c0xffffffff-rj-k-no/photo.jpg" alt="DeadGang" class="img-rounded center-block" />
                <div class="carousel-caption" role="option">
                </div>
            </div>
            <div class="item">
                <img src="~/images/CorpseInClothes.jpg" alt="CorpseInClothes" class="img-rounded center-block" />
                <div class="carousel-caption" role="option">
                </div>
            </div>
            <div class="item">
                <img src="~/images/NeverBetter.png" alt="Never Better!!!" class="img-rounded center-block" />
                <div class="carousel-caption" role="option">
                </div>
            </div>
        </div>
        <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>

    <div class="row">
        <div class="col-md-3">
            <h2 class="text-center bg-primary">twitter</h2>
            <a class="twitter-timeline" href="https://twitter.com/gang_dead?ref_src=twsrc%5Etfw">Tweets by DeadGang</a>
            <script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>
        </div>
        <div class="col-md-3">
            <h2 class="text-center bg-primary">Music</h2>
            <a asp-area="" asp-controller="SoundClouds" asp-action="SoundCloudSongs" class="btn btn-primary center-block">Listen On SoundCloud</a>
            <a asp-area="" asp-controller="SoundClouds" asp-action="Index" class="btn btn-primary center-block">Manage SoundCloud songs</a>
            <h2 class="text-center bg-primary">Events</h2>
            <a asp-area="" asp-controller="Events" asp-action="Calendar" class="btn btn-primary center-block">Calendar</a>
        </div>
        <div class="col-md-3">
            <h2 class="text-center bg-primary">Music Videos</h2>
            <a asp-area="" asp-controller="YouTubeVideos" asp-action="MusicVideos" class="btn btn-primary center-block">Latest Music Videos</a>
            <a asp-area="" asp-controller="YouTubeVideos" asp-action="Index" class="btn btn-primary center-block">Manage Music Videos</a>
        </div>
        <div class="col-md-3">
            <h2 class="text-center bg-primary">Web Shop</h2>
            <a asp-area="" asp-controller="Products" asp-action="Index" class="btn btn-primary center-block">Peruse Our Wares</a>
        </div>
        <div class="row">

        </div>
</body>